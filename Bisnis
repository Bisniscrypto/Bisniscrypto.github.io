        fungsi menghitung() {
         var bnb = document.getElementById("buyinput").value;
          var tokenPerEth = 100000000000;
          var token = tokensPerEth * bnb ;
          console.log(token);
          
         jika(bnb >= 0,5){
            document.getElementById("buyhch2input").value = token*2;
         } lain{
            document.getElementById("buyhch2input").value = token;
         }
        }
    
    
        fungsi refurlspec() {
         
          var bsc = document.getElementById('yourbsc').value;
          console.log(bsc);
          var finalref = document.getElementById("yourbsc").value = "bnbclassic.online/?ref="+ bsc;
    
        }
    
      </skrip>
    
