      <skrip>
        fungsi getreflink() {
          var refaddr = document.getElementById("refadress").InnerText;
          var reflink = document.getElementById("refht");
          reflink.href = '/?ref=' + refaddr;
          reflink.innerHTML = 'bnbclassic.online/?ref=' + refaddr;
          document.getElementById('refs').style.display = 'blok';
        }
        fungsi copyToClipboard(id) {
          var teks = document.getElementById(id).nilai; //mendapatkan teks dari Baris tertentu
          //window.prompt("Salin ke clipboard: Ctrl+C, Enter", teks);
          if (window.clipboardData && window.clipboardData.setData) {
            // IE jalur kode khusus untuk mencegah textarea ditampilkan saat dialog terlihat.
            kembali clipboardData.setData("Teks", teks);
    
          } else if (document.queryCommandSupported && document.queryCommandSupported("copy")) {
            var textarea = document.createElement("textarea");
            textarea.textContent = teks;
            textarea.style.position = "tetap"; // Mencegah pengguliran ke bagian bawah halaman di MS Edge.
            document.body.appendChild(textarea);
            textarea.pilih();
            coba {
              kembali dokumen.execCommand("salin"); // Pengecualian keamanan mungkin dilakukan oleh beberapa browser.
            } tangkap (mis) {
              console.warn("Gagal menyalin ke papan klip.", ex);
              kembali salah;
            } akhirnya {
              document.body.removeChild(textarea);
            }
          }
        }
